<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Cafe Next</title>
<script src='/js/jquery-2.1.1.min.js' type='text/javascript'></script>
</head>

<body>
<div class="container">
<ul id="cafe_list">
</ul>
</div>
</body>
<script>
$(document).ready(function(){
  $.ajax({
    type: "GET",
    url: "http://localhost:9003/cafe/list"
  }).success(function(data) {
    console.log(data.length);
    for (var i = 0; i < data.length; i++) {
        var cafe = data[i];
        console.log(cafe);
        $("#cafe_list").append("<li>" + cafe.name + "</li>");
    }
  });
});
</script>
</html>